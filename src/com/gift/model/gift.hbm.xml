<hibernate-mapping>
    <class name="com.gift.model.GiftVO" table="GIFT" >
        
        <id name="gift_no" column="gift_no">
            <generator class="com.gift.model.GiftGenerator"/> 
        </id>
        
        <property name="gift_name" column="gift_name" />
        <property name="gift_cnt" column="gift_cnt" />
        <property name="gift_price" column="gift_price" />
        <property name="gift_pic" column="gift_pic" />
        <property name="gift_is_on" column="gift_is_on" />
        <property name="gift_track_qty" column="gift_track_qty" />
        <property name="gift_buy_qty" column="gift_buy_qty" />
        
